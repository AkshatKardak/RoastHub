{
  "version": 2,
  "installCommand": "npm install",
  "functions": {
    "api/index.js": {
      "includeFiles": "backend/**"
    }
  },
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
}

